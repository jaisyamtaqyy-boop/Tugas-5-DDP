        "\n",
        "match hitung:\n",
        "    case 1:\n",
        "         sisi = float(input(\"Masukan panjang sisi: \"))\n",
        "         luas = sisi * sisi\n",
        "         print(luas)\n",
        "    case 2:\n",
        "         r = float(input(\"Masukan jari-jari lingkaran: \"))\n",
        "         luas = 3.14 * r * r\n",
        "         print(luas)\n",
        "    case 3:\n",
        "         alas = float(input(\"Masukan ala segitiga: \"))\n",
        "         tinggi = float(input(\"Masukan tinggi segitiga: \"))\n",
        "         luas = 0.5 * alas * tinggi\n",
        "         print(luas)\n",
        "    case _:\n",
        "         print(\"SALAH PILIH\")"
